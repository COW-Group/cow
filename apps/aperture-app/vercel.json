{
  "version": 2,
  "framework": null,
  "installCommand": "echo 'Skipping default install'",
  "buildCommand": "npm ci --legacy-peer-deps && cd apps/aperture-app && npm run build",
  "outputDirectory": "apps/aperture-app/dist",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
