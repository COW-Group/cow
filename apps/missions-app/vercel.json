{
  "version": 2,
  "framework": null,
  "installCommand": "echo 'Skipping default install'",
  "buildCommand": "cp apps/missions-app/.npmrc . && npm ci --legacy-peer-deps && cd apps/missions-app && npm run build",
  "outputDirectory": "apps/missions-app/dist",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
