        dangerouslySetInnerHTML={{
          __html: `
            if (typeof gsap !== 'undefined' && gsap.ScrollTrigger) {
              // Register ScrollTrigger
              gsap.registerPlugin(ScrollTrigger);

              // AuSiri Sophisticated Showcase ScrollTrigger Animation
              const ausiriShowcase = () => {
                const section = document.getElementById('ausiri-sophisticated-showcase');
                if (!section) return;

                const heroTitle = document.getElementById('ausiri-hero-title');
                const heroSubtitle = document.getElementById('ausiri-hero-subtitle');
                const canvasContainer = document.querySelector('.ausiri-canvas-container');
                const stages = document.querySelectorAll('.ausiri-stage');
                const navDots = document.querySelectorAll('.ausiri-nav-dot');

                // Create timeline
                const tl = gsap.timeline({
                  scrollTrigger: {
                    trigger: section,
                    start: 'top top',
                    end: 'bottom bottom',
                    scrub: 1,
                    pin: false,
                    onUpdate: (self) => {
                      const progress = self.progress;
                      const stageIndex = Math.floor(progress * 4);

                      // Update stages
                      stages.forEach((stage, index) => {
                        const opacity = index === stageIndex ? 1 : 0;
                        gsap.set(stage, { opacity, duration: 0.3 });
                      });

                      // Update navigation dots
                      navDots.forEach((dot, index) => {
                        const isActive = index === stageIndex;
                        gsap.set(dot, {
                          opacity: isActive ? 1 : 0.3,
                          scale: isActive ? 1.2 : 1,
                          duration: 0.2
                        });
                      });
                    }
                  }
                });

                // Hero entrance animation
                tl.from(heroTitle, {
                  opacity: 0,
                  y: 100,
                  duration: 1,
                  ease: 'power4.out'
                })
                .from(heroSubtitle, {
                  opacity: 0,
                  y: 50,
                  duration: 0.8,
                  ease: 'power3.out'
                }, '-=0.5');

                // Stage transitions
                tl.to({}, { duration: 2 }) // Traditional gold stage
                  .to({}, { duration: 2 }) // Digital transformation
                  .to({}, { duration: 2 }) // AI optimization
                  .to({}, { duration: 2 }); // Performance results

                // Navigation dot click handlers
                navDots.forEach((dot, index) => {
                  dot.addEventListener('click', () => {
                    const targetProgress = index / 4;
                    gsap.to(window, {
                      scrollTo: {
                        y: section.offsetTop + (targetProgress * section.offsetHeight),
                        autoKill: false
                      },
                      duration: 1.5,
                      ease: 'power3.inOut'
                    });
                  });
                });
              };

              // Initialize when DOM is ready
              if (document.readyState === 'loading') {
                document.addEventListener('DOMContentLoaded', ausiriShowcase);
              } else {
                ausiriShowcase();
              }
            }
          `
        }}
      />

