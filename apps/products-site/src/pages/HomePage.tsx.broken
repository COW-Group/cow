                  opacity: 1,
                  duration: 0.8,
                  ease: 'power2.inOut',
                  delay: 0.4
                })
              }
            }

            // Progress through stages
            const stageInterval = setInterval(progressStages, 3000)

            // Store interval for cleanup
            if (!window.ausiriInterval) {
